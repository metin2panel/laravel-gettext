{
    "name": "depiedra/laravel-gettext",
    "description": "Adds localization support to laravel applications in an easy way using Poedit and GNU gettext.",
    "homepage": "https://github.com/DePiedra/laravel-gettext",
    "keywords": [
        "gettext",
        "localization",
        "poedit",
        "laravel-gettext",
        "laravel", "translation"
    ],
    "license": "proprietary",
    "authors": [
        {
            "name": "Erdem Aksu",
            "email": "eaksu94@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/DePiedra/laravel-gettext/issues"
    },
    "require": {
        "php": "^7.3",
        "laravel/framework": "^8.0",
        "gettext/gettext": "^4.8"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.5",
        "laravel/laravel": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Depiedra\\LaravelGettext\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Depiedra\\LaravelGettext\\ServiceProvider"
            ],
            "aliases": {
                "LaravelGettext": "Depiedra\\LaravelGettext\\Facade"
            }
        }
    }
}
